{
  "name": "meow-boilerplate",
  "version": "1.0.4",
  "description": "It's just a little Meow Boilerplate for your next Project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redeagleap/meow-boilerplate.git"
  },
  "license": "MIT",
  "author": {
    "name": "Alexander Prisyazhnyuk",
    "email": "insightinyourmind@gmail.com"
  },
  "keywords": [
    "meow boilerplate",
    "sass",
    "babel",
    "cypress",
    "webpack",
    "gulp",
    "sassdoc",
    "astrum",
    "boilerplate",
    "grid",
    "layout",
    "framework"
  ],
  "scripts": {
    "assets": "gulp copy:assets --env=init",
    "build": "cross-env NODE_ENV=production npm run subtask:buildgulp && cross-env NODE_ENV=production npm run subtask:bundlewebpack",
    "deploy": "npm run build",
    "dev": "cross-env NODE_ENV=development gulp",
    "e2e": "cypress",
    "e2e:open": "cypress open",
    "favicon": "gulp build:favicon --env=init",
    "fonts": "gulp copy:fonts --env=init",
    "htmlassets": "gulp copy:contentimages --env=init",
    "init": "cross-env NODE_ENV=production gulp init --env=init && cross-env NODE_ENV=production  npm run subtask:bundlewebpack",
    "scripts": "gulp rebuild:js --env=init",
    "stylelint": "stylelint 'src/style/**/**/*.scss'",
    "subtask:buildgulp": "gulp build --env=production",
    "subtask:bundlewebpack": "cross-env NODE_ENV=production webpack --config=webpack/webpack.prod.babel.js -p --colors --mode=production",
    "template": "gulp compiler:html --env=init",
    "prettify": "prettier prettier --loglevel=warn --write \"{webpack,src}/**/*.{js,scss}\"",
    "uiimages": "gulp rebuild:images --env=init",
    "webpack:analyze": "webpack-bundle-analyzer webpack/stats.json dist/assets/"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "baukasten-grid": "^1.0.16",
    "bowser": "^2.0.0-beta.1",
